{
  "name": "locli",
  "version": "0.8.0",
  "description": "CLI for managing json files with similar structure in a batch ",
  "main": "lib",
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./lib/** && tsc --outDir ./lib/ --importHelpers",
    "start": "yarn lint && yarn build",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "yarn start"
  },
  "bin": {
    "locli": "./lib/index.js"
  },
  "keywords": [
    "cli",
    "json",
    "locale",
    "i18n",
    "tool"
  ],
  "author": "Igor Ovsiannikov <garek@mail.ru>",
  "license": "ISC",
  "dependencies": {
    "commander": "^2.9.0",
    "lodash": "^4.17.4",
    "pify": "^2.3.0",
    "tslib": "^1.6.0"
  },
  "devDependencies": {
    "@types/commander": "^2.3.31",
    "@types/lodash": "^4.14.61",
    "@types/node": "^7.0.12",
    "@types/pify": "^0.0.28",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2"
  }
}
